<% if @subscribe.errors.any? %>
  $('.errors').html('<%= j render 'shared/errors', object: @subscribe %>');
<% else %>
  $('.flash').html('<%= j render 'shared/flash' %>');
  $('.subscribe-btn').replaceWith('<%= j link_to 'Unsubscribe', subscribe_path(@subscribe), method: :delete, remote: true, class: 'unsubscribe-btn' %>')
<% end %>